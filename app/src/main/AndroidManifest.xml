<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rodrigo.anotadoruniversal">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Truco"></activity>
        <activity android:name=".Escoba"></activity>
        <activity android:name=".Chancho"></activity>
        <activity android:name=".Generala"></activity>
        <activity android:name=".Chinchon"></activity>
        <activity android:name=".InformacionClase"
            android:theme="@style/Theme.AppCompat.Dialog"></activity>
        <activity android:name=".ConfiguracionTruco"></activity>
        <activity android:name=".TrucoGallo"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>
</manifest>